# EditorConfig is awesome: https://github.com/editorconfig/editorconfig/wiki/EditorConfig-Properties
# Refer: https://github.com/iuap-design/blog/issues/189

# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
[*]
end_of_line = lf
insert_final_newline = true
# 删除行尾空格
trim_trailing_whitespace = true

[*.java]
# TIPS set customize extension
charset = utf-8
indent_style = space
indent_size = 2
