<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <display-name>SSEExhibition</display-name>
    <servlet>
        <servlet-name>GenKeyServlet</servlet-name>
        <servlet-class>org.ruiyun.Servlet.GenKeyServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GenKeyServlet</servlet-name>
        <url-pattern>/GenKeyServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>IEX2levServlet</servlet-name>
        <servlet-class>org.ruiyun.Servlet.IEX2levServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>IEX2levServlet</servlet-name>
        <url-pattern>/IEX2levServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>IEXZMFServlet</servlet-name>
        <servlet-class>org.ruiyun.Servlet.IEXZMFServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>IEXZMFServlet</servlet-name>
        <url-pattern>/IEXZMFServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>UploadServlet</servlet-name>
        <servlet-class>org.ruiyun.Servlet.UpLoadServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UploadServlet</servlet-name>
        <url-pattern>/UploadServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>EliFileServlet</servlet-name>
        <servlet-class>org.ruiyun.Servlet.EliFileServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>EliFileServlet</servlet-name>
        <url-pattern>/EliFileServlet</url-pattern>
    </servlet-mapping>

    <welcome-file-list>
        <welcome-file>main.jsp</welcome-file>
    </welcome-file-list>
</web-app>
